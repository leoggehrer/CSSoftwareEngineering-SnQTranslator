@*@CodeCopy*@
@*This partial view can alternatively be used with the shared index view.*@
@*#if ACCOUNT_ON*@
@{
    @using SnQTranslator.AspMvc.Modules.Language
    @model IEnumerable<SnQTranslator.AspMvc.Models.Business.Account.IdentityUser>

    var controller = ViewContext.RouteData.Values["controller"].ToString();
    var action = ViewContext.RouteData.Values["action"].ToString();
    Func<string, string> translate = Translator.TranslateIt;
    Func<string, string> translateFor = pn => translate($"{controller}.{pn}");

    <tr>
        <th>
            @translateFor(Html.DisplayNameFor(model => model.OneItem.Name))
        </th>
        <th>
            @translateFor(Html.DisplayNameFor(model => model.OneItem.Email))
        </th>
        <th>
            @translateFor(Html.DisplayNameFor(model => model.AnotherItem.Lastname))
        </th>
        <th>
            @translateFor(Html.DisplayNameFor(model => model.AnotherItem.Firstname))
        </th>
        <th>
            @translateFor(Html.DisplayNameFor(model => model.OneItem.EnableJwtAuth))
        </th>
        <th>
            @translateFor(Html.DisplayNameFor(model => model.OneItem.AccessFailedCount))
        </th>
        <th>
            @translateFor(Html.DisplayNameFor(model => model.OneItem.State))
        </th>
        <th></th>
    </tr>
}
@*#endif*@
